<div id="<%= dom_id interaction %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Customer:</strong>
    <%= interaction.customer_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= interaction.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= interaction.company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Started at:</strong>
    <%= interaction.started_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ended at:</strong>
    <%= interaction.ended_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Recording url:</strong>
    <%= interaction.recording_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Transcript:</strong>
    <%= interaction.transcript %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Memo:</strong>
    <%= interaction.memo %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Questionnaire data:</strong>
    <%= interaction.questionnaire_data %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= interaction.status %>
  </div>
</div>
